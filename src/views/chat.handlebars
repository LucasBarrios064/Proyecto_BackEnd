<div>
    <h1>Online Chat</h1>


    <div>
        <form action="" method="post">
        <input type="text" placeholder="email" id="inputEmail" required>
        <input type="text" placeholder="message" id="inputMessage" required>
        <button id="sumbitEmail" type="submit">Enviar</button>
        </form>
    </div>

    <div>
        <p>Mensajes</p>
        <div id="messageRender">
            {{#each messageReder}}
                <p> <strong> {this.email} </strong> {this.message} <br> </p>
            {{/each}}
        </div>
    </div>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/chat.js"></script>