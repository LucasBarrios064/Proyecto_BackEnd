<div>
  <h1>Lista de los Productos</h1>
  <div>
    <ul>
      {{#each docs}}
        <li>
          <h3>{{this.title}}</h3>
          <p>Id: {{this.id}} </p>
          <p>Description: {{this.description}} </p>
          <p>Code: {{this.code}} </p>
          <p>Price: ${{this.price}} </p>
          <p>Status: {{this.status}} </p>
          <p>Category: {{this.category}} </p>
          <p>Thumbnail: {{this.thumbnail}} </p>
        </li>
      {{/each}}
      </ul>
  </div>
  {{#if prevPage}}
  <a href="/pagination?page={{prevPage}}">Anterior</a>
{{/if}}
{{page}}
{{#if nextPage}}
  <a href="/pagination?page={{nextPage}}">Siguiente</a>
{{/if}}
</div>